@model IEnumerable<string>

@if (Model != null && Model.Any())
{
    <div class="alert alert-danger" role="alert" aria-live="polite">
        <h4 class="alert-heading">Erros encontrados</h4>
        <ul class="mb-0">
            @foreach (var error in Model)
            {
                <li>@error</li>
            }
        </ul>
    </div>
}
